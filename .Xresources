URxvt.termName: rxvt-256color

URxvt.depth: 32
URxvt.foreground: snow
URxvt.background: black
! Blue
URxvt.color4: #5566FF
! Bright blue
URxvt.color12: #7799FF

URxvt.transparent: true
URxvt.shading: 15

! Disable scrollbar
URxvt.scrollBar: false

! URxvt.font: xft:Ricty Discord:Regular:size=10:antialias=true
URxvt.font:\
    xft:Ricty Discord:Regular:size=9:antialias=true,
    xft:Hiragino Maru Gothic Pro W4:size=9:antialias=true
!URxvt.letterSpace: -5


! Copy and paste
URxvt.perl-ext-common: url-select,clipboard
! left click to open URL
URxvt.url-select.button: 1
! underline URLs
URxvt.url-select.underline: true
URxvt.keysym.C-C: perl:clipboard:copy
URxvt.keysym.C-V: perl:clipboard:paste
