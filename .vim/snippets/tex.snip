# vim: noexpandtab
snippet		helloworld
abbr		hello world
options		head
	\documentclass{article}

	\begin{document}
		Hello world!
	\end{document}

snippet		aligned
abbr		aligned
	\begin{aligned}
		${0}
	\end{aligned}

snippet		frac
abbr		frac
	\frac{${1}}{${2}}

snippet		label_alpha
abbr		[label=(\alph*)]
	[label=(\alph*)]

snippet		bibliography
	\bibliographystyle{plain}
	\bibliography{${1:bibliography}}
	${0}
